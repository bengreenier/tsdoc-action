{
  "name": "tsdoc-action",
  "version": "1.0.0",
  "description": "GitHub Action to build typedoc documentation",
  "author": {
    "name": "Erik Golinelli",
    "email": "erik@codekraft.it",
    "url": "https://github.com/erikyo"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/erikyo/tsdoc-action/issues"
  },
  "homepage": "https://github.com/erikyo/tsdoc-action#readme",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/index.ts -o lib --source-map --license LICENSE",
    "lint": "eslint src --fix",
    "tsdoc": "npx typedoc src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikyo/tsdoc-action.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "ts doc",
    "typedoc",
    "Documentation",
    "JavaScript",
    "TypeScript"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "typedoc": "^0.25.4"
  },
  "devDependencies": {
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3"
  }
}
